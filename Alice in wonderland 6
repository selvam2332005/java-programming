import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int n = sc.nextInt();
        int path = (n/10)+(n%10);
        System.out.println("Bird said:"+n);
        System.out.println("Alice must go in path-"+path);
        
    }
}
